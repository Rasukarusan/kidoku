{
  "name": "kidoku",
  "private": true,
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.79",
      "@types/react-dom": "18.2.25"
    }
  },
  "scripts": {
    "build": "turbo run build",
    "build:libraries": "pnpm --filter @kidoku/database build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "validate": "turbo run lint check-types test",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.5.1",
    "typescript": "5.8.2"
  },
  "engines": {
    "pnpm": ">=10.5.2"
  },
  "packageManager": "pnpm@10.5.2"
}
